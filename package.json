{
  "name": "@djie/eslint-config-monorepo",
  "version": "1.0.22",
  "private": true,
  "packageManager": "pnpm@8.1.1",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "prepare": "pnpm -r run stub",
    "release": "bumpp -r && pnpm -r publish"
  },
  "devDependencies": {
    "@djie/eslint-config": "workspace:*",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "eslint-plugin-antfu": "^0.38.4",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "typescript"
      ]
    }
  }
}
