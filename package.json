{
  "name": "@djie/eslint-config-monorepo",
  "version": "1.0.8",
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public --registry https://registry.npmjs.org/ --no-git-checks"
  },
  "devDependencies": {
    "@djie/eslint-config": "workspace:*",
    "bumpp": "^7.2.0",
    "eslint": "^8.18.0",
    "eslint-plugin-antfu": "^0.25.2",
    "typescript": "^4.7.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "typescript"
      ]
    }
  }
}
